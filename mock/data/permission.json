[
  {
    "path": "/",
    "component": "Layout",
    "redirect": "/dashboard",
    "children": [
      {
        "name": "dashboard",
        "path": "dashboard",
        "component": "dashboard/index",
        "meta": {
          "title": "首页",
          "affix": true,
          "icon": "house"
        }
      }
    ]
  },
  {
    "path": "/auth",
    "component": "Layout",
    "meta": {
      "title": "权限管理",
      "icon": "auth"
    },
    "children": [
      {
        "name": "group",
        "path": "group",
        "component": "auth/group",
        "meta": {
          "title": "角色组管理",
          "icon": "auth"
        }
      },
      {
        "name": "admin",
        "path": "admin",
        "component": "auth/admin",
        "meta": {
          "title": "管理员管理",
          "icon": "user"
        }
      },
      {
        "name": "adminLog",
        "path": "adminLog",
        "component": "auth/adminLog",
        "meta": {
          "title": "管理员日志管理",
          "icon": "log"
        }
      }
    ]
  },
  {
    "path": "/user",
    "component": "Layout",
    "meta": {
      "title": "用户管理",
      "icon": "user"
    },
    "children": [
      {
        "name": "user",
        "path": "index",
        "component": "user/index",
        "meta": {
          "title": "用户列表",
          "icon": "user"
        }
      }
    ]
  },
  {
    "path": "/shop",
    "component": "Layout",
    "meta": {
      "title": "商城管理",
      "icon": "shop"
    },
    "children": [
      {
        "name": "shop",
        "path": "index",
        "component": "shop/index",
        "meta": {
          "title": "商品列表",
          "icon": "shop"
        }
      },
      {
        "name": "shop-goods-publish",
        "path": "shop-goods-publish",
        "component": "shop/shopGoodsPublish",
        "hidden": true,
        "meta": {
          "title": "商品发布"
        }
      },
      {
        "name": "shop-order",
        "path": "shop-order",
        "component": "shop/shopOrder",
        "meta": {
          "title": "订单列表",
          "icon": "shop-order"
        }
      },
      {
        "name": "shop-order-details",
        "path": "shop-order-details",
        "component": "shop/shopOrderDetails",
        "hidden": true,
        "meta": {
          "title": "订单详情"
        }
      }
    ]
  },
  {
    "path": "/routine",
    "component": "Layout",
    "meta": {
      "title": "常规管理",
      "icon": "routine"
    },
    "children": [
      {
        "name": "config",
        "path": "config",
        "component": "routine/config",
        "meta": {
          "title": "系统配置",
          "icon": "set"
        }
      },
      {
        "name": "adminInfo",
        "path": "adminInfo",
        "component": "routine/adminInfo",
        "meta": {
          "title": "个人资料",
          "icon": "admininfo"
        }
      }
    ]
  },
  {
    "path": "/security",
    "component": "Layout",
    "meta": {
      "title": "数据安全管理",
      "icon": "security"
    },
    "children": [
      {
        "name": "dataRecycleLog",
        "path": "dataRecycleLog",
        "component": "security/dataRecycleLog",
        "meta": {
          "title": "数据回收站",
          "icon": "data-recycle-log"
        }
      },
      {
        "name": "sensitiveDataLog",
        "path": "sensitiveDataLog",
        "component": "security/sensitiveDataLog",
        "meta": {
          "title": "敏感数据修改记录",
          "icon": "sensitive-data-log"
        }
      }
    ]
  }
]